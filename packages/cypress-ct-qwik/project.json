{
  "name": "cypress-ct-qwik",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "root": "packages/cypress-ct-qwik",
  "sourceRoot": "packages/cypress-ct-qwik/src",
  "projectType": "library",
  "targets": {
    "definition": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --project ./packages/cypress-ct-qwik/tsconfig.definition.json"
      }
    },
    "mount": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --project ./packages/cypress-ct-qwik/tsconfig.mount.json"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": " mkdir -p ./apps/qwik-app/node_modules && cp -r ./packages/cypress-ct-qwik ./apps/qwik-app/node_modules/cypress-ct-qwik"
      },
      "outputPath": "node_modules/cypress-ct-qwik",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["definition", "mount"]
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs cypress-ct-qwik {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/cypress-ct-qwik/**/*.ts"]
      }
    }
  },
  "tags": []
}

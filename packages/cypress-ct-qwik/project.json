{
  "name": "cypress-ct-qwik",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "root": "packages/cypress-ct-qwik",
  "sourceRoot": "packages/cypress-ct-qwik/src",
  "projectType": "library",
  "targets": {
    "prebuild": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/cypress-ct-qwik",
        "tsConfig": "packages/cypress-ct-qwik/tsconfig.lib.json",
        "main": "packages/cypress-ct-qwik/src/index.ts",
        "assets": [
          {
            "input": "./assets",
            "glob": "cypress-qwik.png",
            "output": "./"
          },
          {
            "input": "./",
            "glob": "README.md",
            "output": "./"
          },
          {
            "input": "./",
            "glob": "CONTRIBUTING.md",
            "output": "./"
          },
          {
            "input": "./",
            "glob": "CODE_OF_CONDUCT.md",
            "output": "./"
          },
          {
            "input": "./",
            "glob": "LICENSE",
            "output": "./"
          }
        ]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": ["^build", "prebuild"],
      "options": {
        "commands": [
          "cp -r dist/packages/cypress-ct-qwik node_modules",
          "cp -r dist/packages/cypress-ct-qwik-definition/src node_modules/cypress-ct-qwik",
          "cp -r dist/packages/cypress-ct-qwik-mount/src node_modules/cypress-ct-qwik"
        ]
      },
      "outputPath": "node_modules/cypress-ct-qwik",
      "outputs": ["{options.outputPath}"]
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs cypress-ct-qwik {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/cypress-ct-qwik/**/*.ts"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "cypress-ct-qwik-mount",
    "cypress-ct-qwik-definition"
  ]
}

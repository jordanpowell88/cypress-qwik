{
  "name": "cypress-ct-qwik",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/cypress-ct-qwik/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "node_modules/cypress-ct-qwik",
        "main": "packages/cypress-ct-qwik/src/index.ts",
        "tsConfig": "packages/cypress-ct-qwik/tsconfig.lib.json",
        "assets": [
          {
            "input": "./assets",
            "glob": "cypress-qwik.png",
            "output": "./assets"
          },
          {
            "input": "./",
            "glob": "README.md",
            "output": "./"
          },
          {
            "input": "./",
            "glob": "CONTRIBUTING.md",
            "output": "./"
          },
          {
            "input": "./",
            "glob": "CODE_OF_CONTACT.md",
            "output": "./"
          },
          {
            "input": "./",
            "glob": "LICENSE",
            "output": "./"
          }
        ]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs cypress-ct-qwik {args.ver} {args.tag}"
      },
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/cypress-ct-qwik/**/*.ts"]
      }
    }
  },
  "tags": []
}
